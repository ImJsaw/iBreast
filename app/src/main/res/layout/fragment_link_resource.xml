<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:tag="link_center">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/hair_showArea"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/row_shape"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/row_shape">
                    <TextView
                        android:layout_width="382dp"
                        android:layout_height="30dp"
                        android:background="@drawable/north_shape"
                        android:padding="5dp"
                        android:scrollHorizontally="false"
                        android:textAlignment="center"
                        android:text="@string/hair"/>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/row_shape">
                    <TextView
                        android:id="@+id/link_name_hair"
                        android:layout_width="130dp"
                        android:layout_height="30dp"
                        android:background="@drawable/top_shape"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:scrollHorizontally="false"
                        android:text="@string/link_name"
                        android:textAlignment="center" />

                    <TextView
                        android:id="@+id/link_phone_hair"
                        android:layout_width="105dp"
                        android:layout_height="30dp"
                        android:layout_toEndOf="@id/link_name_hair"
                        android:background="@drawable/top_shape"
                        android:ems="10"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:text="@string/link_phone"
                        android:textAlignment="center" />

                    <TextView
                        android:id="@+id/link_address_hair"
                        android:layout_width="147dp"
                        android:layout_height="30dp"
                        android:layout_toEndOf="@id/link_phone_hair"
                        android:background="@drawable/top_shape"
                        android:ems="10"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:text="@string/link_address"
                        android:textAlignment="center" />
                </RelativeLayout>
            </LinearLayout>
            <TableLayout
                android:id="@+id/hair_hide_table"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/row_shape" />

            <LinearLayout
                android:id="@+id/bra_showArea"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/row_shape"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/row_shape">
                    <TextView
                        android:layout_width="382dp"
                        android:layout_height="30dp"
                        android:background="@drawable/central_shape"
                        android:padding="5dp"
                        android:scrollHorizontally="false"
                        android:textAlignment="center"
                        android:text="@string/dummy_bra"/>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/row_shape">
                    <TextView
                        android:id="@+id/link_name_bra"
                        android:layout_width="130dp"
                        android:layout_height="30dp"
                        android:background="@drawable/top_shape"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:scrollHorizontally="false"
                        android:text="@string/link_name"
                        android:textAlignment="center" />

                    <TextView
                        android:id="@+id/link_phone_bra"
                        android:layout_width="105dp"
                        android:layout_height="30dp"
                        android:layout_toEndOf="@id/link_name_bra"
                        android:background="@drawable/top_shape"
                        android:ems="10"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:text="@string/link_phone"
                        android:textAlignment="center" />

                    <TextView
                        android:id="@+id/link_address_bra"
                        android:layout_width="147dp"
                        android:layout_height="30dp"
                        android:layout_toEndOf="@id/link_phone_bra"
                        android:background="@drawable/top_shape"
                        android:ems="10"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:text="@string/link_address"
                        android:textAlignment="center" />
                </RelativeLayout>
            </LinearLayout>
            <TableLayout
                android:id="@+id/bra_hide_table"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/row_shape" />

            <LinearLayout
                android:id="@+id/cuff_showArea"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/row_shape"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/row_shape">
                    <TextView
                        android:layout_width="382dp"
                        android:layout_height="30dp"
                        android:background="@drawable/south_shape"
                        android:padding="5dp"
                        android:scrollHorizontally="false"
                        android:textAlignment="center"
                        android:text="@string/cuff"/>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/row_shape">
                    <TextView
                        android:id="@+id/link_name_cuff"
                        android:layout_width="130dp"
                        android:layout_height="30dp"
                        android:background="@drawable/top_shape"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:scrollHorizontally="false"
                        android:text="@string/link_name"
                        android:textAlignment="center" />

                    <TextView
                        android:id="@+id/link_phone_cuff"
                        android:layout_width="105dp"
                        android:layout_height="30dp"
                        android:layout_toEndOf="@id/link_name_cuff"
                        android:background="@drawable/top_shape"
                        android:ems="10"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:text="@string/link_phone"
                        android:textAlignment="center" />

                    <TextView
                        android:id="@+id/link_address_cuff"
                        android:layout_width="147dp"
                        android:layout_height="30dp"
                        android:layout_toEndOf="@id/link_phone_cuff"
                        android:background="@drawable/top_shape"
                        android:ems="10"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:text="@string/link_address"
                        android:textAlignment="center" />
                </RelativeLayout>
            </LinearLayout>
            <TableLayout
                android:id="@+id/cuff_hide_table"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/row_shape" />

            <LinearLayout
                android:id="@+id/home_showArea"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/row_shape"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/row_shape">
                    <TextView
                        android:layout_width="382dp"
                        android:layout_height="30dp"
                        android:background="@drawable/east_shape"
                        android:padding="5dp"
                        android:scrollHorizontally="false"
                        android:textAlignment="center"
                        android:text="@string/home_service"/>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/row_shape">
                    <TextView
                        android:id="@+id/link_name_home"
                        android:layout_width="130dp"
                        android:layout_height="30dp"
                        android:background="@drawable/top_shape"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:scrollHorizontally="false"
                        android:text="@string/link_name"
                        android:textAlignment="center" />

                    <TextView
                        android:id="@+id/link_phone_home"
                        android:layout_width="105dp"
                        android:layout_height="30dp"
                        android:layout_toEndOf="@id/link_name_home"
                        android:background="@drawable/top_shape"
                        android:ems="10"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:text="@string/link_phone"
                        android:textAlignment="center" />

                    <TextView
                        android:id="@+id/link_address_home"
                        android:layout_width="147dp"
                        android:layout_height="30dp"
                        android:layout_toEndOf="@id/link_phone_home"
                        android:background="@drawable/top_shape"
                        android:ems="10"
                        android:maxLines="5"
                        android:padding="5dp"
                        android:text="@string/link_address"
                        android:textAlignment="center" />
                </RelativeLayout>
            </LinearLayout>
            <TableLayout
                android:id="@+id/home_hide_table"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/row_shape" />

        </LinearLayout>
    </ScrollView>



</FrameLayout>
